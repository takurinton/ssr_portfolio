{
  "name": "ssr_portfolio",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && npm run dev",
    "dev": "NODE_ENV=production node ./assets/server/server.js",
    "build": "rm -rf dist/ && npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.client.config.js --mode development",
    "build:server": "rm -rf ./assets/ && babel src --config-file ./babel.config.js  -x '.ts,.tsx' -d assets",
    "build:server:webpack": "webpack --config webpack.server.config.js --mode development",
    "build:style": "node-sass ./src/styles --output ./dist/css/",
    "clear": "rm -rf ./dist/ && rm -rf ./assets/"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@linaria/babel-preset": "^3.0.0-beta.1",
    "@linaria/core": "^3.0.0-beta.1",
    "@linaria/react": "^3.0.0-beta.1",
    "@linaria/shaker": "^3.0.0-beta.1",
    "@linaria/webpack-loader": "^3.0.0-beta.1",
    "@types/marked": "^1.2.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-scss": "^1.0.9",
    "babel-preset-minify": "^0.5.1",
    "core-js": "^3.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "history": "^5.0.0",
    "linaria": "^3.0.0-beta.1",
    "marked": "^1.2.7",
    "mini-css-extract-plugin": "^1.3.4",
    "node-fetch": "^2.6.1",
    "node-sass-middleware": "^0.11.0",
    "postcss-loader": "^4.1.0",
    "react": "^17.0.1",
    "react-css-modules": "^4.7.11",
    "react-dom": "^17.0.1",
    "react-highlight": "^0.13.0",
    "react-highlight.js": "^1.0.7",
    "sass": "^1.32.4",
    "ts-loader": "^8.0.14",
    "tslint-loader": "^3.5.4",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "css-modules-typescript-loader": "^2.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1"
  }
}
