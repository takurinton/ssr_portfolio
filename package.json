{
  "name": "ssr_portfolio",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && npm run dev",
    "dev": "NODE_ENV=production node ./assets/server/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "rm -rf ./dist/ && webpack --config webpack.client.config.js --mode development",
    "build:server": "rm -rf ./assets/ && babel src -x '.ts,.tsx' -d assets",
    "clear": "rm -rf ./dist/ && rm -rf ./assets/"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/marked": "^1.2.1",
    "core-js": "^3.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "linaria": "^3.0.0-beta.1",
    "marked": "^1.2.7",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-highlight": "^0.13.0",
    "react-highlight.js": "^1.0.7",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "ts-loader": "^8.0.14",
    "tslint-loader": "^3.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-node-externals": "^2.5.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.0"
  }
}
