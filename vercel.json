{
    "builds": [
        {
            "src": "api/server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/client.css", 
            "dest": "/client.css"
        }, 
        {
            "src": "/(.*)",
            "dest": "api/server/server.js"
        }
    ]
}